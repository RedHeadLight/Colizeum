mixin clubsList(modificator, extraClasses, count)
	- classList = 'clubsList'

	if modificator
		each mod in modificator
			- classList = classList + ' clubsList--' + mod

	if extraClasses
		- classList = classList + ' ' + extraClasses

	div(class=classList)
		.clubsList__list 
			- var i = 0
			while i < count 
				- i = i + 1
				.clubsList__item 
